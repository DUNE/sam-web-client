#! /usr/bin/python -E

import os,sys

if __name__ == '__main__':
    # fix up path to find the code
    ourpath = sys.path[0]
    sys.path.insert(0, os.path.normpath(os.path.join(ourpath, '../python')))

    import samweb_cli
    try:
        sys.exit(samweb_cli.main())
    except KeyboardInterrupt:
        sys.exit(130)

